OnCheckGlitch
{
	_glitches = FENUM('logs/@$a#F.pg','+')
	--
	if _glitches == 0 || "M-201z.txt" !_in_ _glitches
	{
		repairglitch = 1
		quest = 2

		"\b[4]S̴e̵e̸m̷ like it worked,\w2 thanks,\w1 now communication will be easier \w8. \w8. \w8. I think I should use some type of formating for my text so you can distinguish between me and %(devname)\nWhat do you think I should use?\n"
		--
		"\![*]\q[Bold text,OnDisting,bold]\n\![*]\q[Cursive text,OnDisting,cursive]\n/
		\![*]\q[Striked text,OnDisting,strike]\n\![*]\q[Underlined text,OnDisting,underline]\n/
		\![*]\q[Yellow text,OnDisting,yellow]\n\![*]\q[Green text,OnDisting,green]\n/
		\![*]\q[Red text,OnDisting,red]\n\![*]\q[Pink text,OnDisting,pink]\n"
	}

}

OnDisting
{
	distingtype = reference0
	--
	case distingtype
	{
		when "bold"
		{
			distingstart = "\f[bold,1]"
		}
		when "cursive"
		{
			distingstart = "\f[italic,1]"
		}
		when "strike"
		{
			distingstart = "\f[strike,1]"
		}
		when "underline"
		{
			distingstart = "\f[color,1]"
		}
		when "yellow"
		{
			distingstart = "\f[color,#fffa6b]"
		}
		when "green"
		{
			distingstart = "\f[color,#1bcc2a]"
		}
		when "red"
		{
			distingstart = "\f[color,#e62719]"
		}
		when "pink"
		{
			distingstart = "\f[color,#ffa1f7]"
		}
		when "custom"
		{
			"%(distingstart)Oh, can you copy the hex of a color you want to use?\![open,inputbox,OnCustomColor,-1]"
		}
		
	}
	--
	if formattype == 1
	{
		formattype = 0
		TalkConfig
	}
	else
	{
		"So %(distingtype) huh, well ok\xSo now I will talk%(distingstart) like this\e"
	}
	
}


OnCustomColor
{
	distingtype = reference0
	--
	if "#" !_in_ reference0
	{
		distingstart = "\f[color,#"+ reference0 +"]"
	}
	else
	{
		distingstart = "\f[color,"+ reference0 +"]"
	}
	--
	if formattype == 1
	{
		formattype = 0
		TalkConfig
	}
	else
	{
		"So %(distingtype) huh, well ok\xSo now I will talk%(distingstart) like this\e"
	}
}

