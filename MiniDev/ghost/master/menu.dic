Select.delpage
{
	if pagenum != 0
	{
		for _i=pagenum;_i<todopagenum;_i++
		{
			taskstatus[_i] = taskstatus[_i+1]
			tasks[_i] = tasks[_i+1]
			namepage[_i] = namepage[_i+1]
			taskamount[_i] = taskamount[_i+1]
		}
		--
		todopagenum--
		--
		if pagenum < todopagenum 
		{
			pagenum++
		}
		else
		{
			pagenum--
		}
	}
	else
	{
		taskstatus[pagenum] = 0
		tasks[pagenum] = ""
		namepage[pagenum] = ""
		taskamount[_i] = 0
	}
	--
	Select.todotwooo
}

