	}
	else
	{
		remindpage += "\n\n[half]Use Default Ringtone:  \f[color,#1bcc2a]\q[%(usedefring),defringuse]\f[color,default]"
	}
	--
	if usedefring == "ON"
	{
		if defring == ""
