OnTranslate
{
	_talk = reference0
	--
	if nowshell != "M-201z"
	{
		_talk = RE_REPLACE(_talk,"(\\i\[\d*\])","")
	}
	--
	if colors == "OFF"
	{
		//_talk = RE_REPLACE(_talk,"(\\f\[color\,\#\w*\])","")
	}
	--
	if fontsize != 15
	{
		//_talk = "\f[height," + fontsize + "]" + _talk
	}
	--
	if david != 1
	{
		_talk = REPLACE(_talk, "DAVD", "\_a[david]David\_a")
	}
	else
	{
		_talk = REPLACE(_talk, "DAVD", "David")
	}
	--
	if othermz != 1
	{
		_talk = REPLACE(_talk, "OTMZ", "\_a[M-z]other M-z\_a")
	}
	else
	{
		_talk = REPLACE(_talk, "OTMZ", "other M-z")
	}
	--
	if oldntbk != 1
	{
		_talk = REPLACE(_talk, "OLDNTBK", "\_a[oldnotebook]oldnotebook\_a")
	}
	else
	{
		_talk = REPLACE(_talk, "OLDNTBK", "oldnotebook")
	}
	--
	if reference1 == "" && reference2 == "" 
	{ 
		EVAL('"' + REPLACE(_talk,'"','" + CHR(0x22) + "') + '"') //'"
	}
	else
		{ 
		TOSTR(_talk)
	}
}

OnAnchorSelect
{
    "%(pattext)"
    --
	if "http://" _in_ reference0 
	{
		"\j[%(reference0)]\e" 
	}
	else 
	{
		case reference0 
		{
			when "M-z"
			{
				othermz = 1
				mood[5] += 2
				mood[2] ++
				--
