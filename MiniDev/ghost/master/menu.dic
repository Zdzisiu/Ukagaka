//OX游戏，by Pygmalion, loosely translated by Zar
//edited some by vita to have the AI make mistakes
OnPlayOX
{
    //intializing a tictactoe array with all 0's
    arrOX = IARRAY
    for _i = 0; _i < 9; _i++ {
        arrOX = (arrOX, 0)
    }
    --
    PlayOXshow(0)
}

PlayOXshow
{
	games++
	m201relationship++
	--
	if mood[1] >= 0
	{
		mood[1]--
	}
	--
    //this function prints out the tictactoe array
    _tempstr = ""
    
    for _i = 0; _i < 3; _i++ {
        
        for _j = 0; _j < 3; _j++ {
            
            _temp = _i * 3 + _j
            
            
            if arrOX[_temp] == -1
