MDtalk
{
	if smart == "ON"
	{
		"%(didknow) %(facts)?"
		{
			_binary = binbin
			_letterB = _binary[0] 
			_bcode = _binary[1] 
			"\0In binary '%(_letterB)' is '%(_bcode)'"
			"\0In binary '%(_bcode)' is '%(_letterB)'"
		}

		{
			_morse = morse
			_letterm = _morse[0] 
			_mcode = _morse[1] 
			"\0In morse '%(_letterm)' is '%(_mcode)'"
			"\0In morse '%(_mcode)' is '%(_letterm)'"
		}
	}

	if stupid == "ON"
	{
		"%(didknow) %(myths)?"
		"%(myths)"
	}

	if RAND(50) > 30
	{
		"remember to take breaks from your computer, it's not healthy to sit like that for long periods of time"
	}
	
	if romantic == "ON"
	{
		"%(pickupline)"
	}

	{
		if lefttask[pagenum] != 0
		{
			if lefttask[pagenum] == 1
			{
				"You still have 1 task to do on '%(namepage[pagenum])' list"
			}
			else
			{
				"You still have %(lefttask[pagenum]) tasks to do on '%(namepage[pagenum])' list"
			}

			{
				randtask = RAND(taskamount[pagenum])
				--
				if randtask == 0
				{
					randtask = 1
				}
				--
				for x=0,_i=0;x==0 || _i<=taskamount[pagenum];_i++
				{
					if pagetaskstatus[_i] != 1
					{
						x = 1
					}
					else
					{
						randtask = RAND(taskamount[pagenum])
					}
					
				}
				--
				if pagetasks[randtask] != "" 
				{
					"You should try completing one of the tasks, maybe try completing '%(pagetasks[randtask])' from '%(namepage[pagenum])'"
				}
			}
		}
	}
}

