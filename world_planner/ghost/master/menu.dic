OnOpenIMG
{
	appthing = reference0
	appthing2 = reference1
	--
	if delapp == "ON"
	{
		FDEL("images/%(reference1)")
		--
		"\c"
	}
	elseif editap == "ON"
	{
		"\![open,inputbox,OnReImg,-1]"
	}
	else
	{
		"\![open,file,images/%(reference1)]"
	}
	--
	Select.imglib
}

OnReImg
{
	editap = "OFF"
	appre = SPLIT(appthing2,".")
	FRENAME("images/%(appre[0]).%(appre[1])","images/" +reference0 + "." + appre[1])
	--
	"\c"
	--
	Select.imglib
}
