OnAskOmega
{
	case reference0
	{
		when "imgs"
		{
			"\b2\_qIf you want me to save an image you have to drop it on me\n\n[half]If you want to upload a character portrait name the image the same as character\n\n[half]If you want to save a map name the image like the country or add '_map'\n\n[half]If you want to save a reference sheet of a race name it the same as the said race\n\n[half]Any other images i can save in one of my books if you want me to do it\_q"
		}
		when "law"
		{
			"\_qTo delete a law click on 'remove law' and then on the number of the law you want to delete\_q"
		}
		when "him"
		{
			"I'm a librarian"
			"I'm a guardian"
			"I'm gods servant"
		}
		when "age"
		{
			"Age doesn't matter when you are already dead"
			"Guardians live outside of time"
			"I don't know but it doesn't really matter"
		}
	}
	--
	"\x\c"
	--
	Select.askomega

}
//======================================IMAGE LIBRARY==============================================
Select.imglib
{
	imglist = ""
	imglist +="\f[bold,1]\_q\b[4]Your images\f[bold,0]:\n\n"
	--
	_imgs = FENUM('images','+')
	if _imgs == ""
	{
		imglist +="\f[bold,1]You don't have any images\f[bold,0]\n"
	}
	else
	{
		_trueimg = SPLIT(_imgs,'+')
		_imgs = REPLACE(_imgs,'.png','')
		_imgs = REPLACE(_imgs,'.jpg','')
		_imgs = REPLACE(_imgs,'.jpeg','')
		_imgs = REPLACE(_imgs,'.bmp','')
		_imgs = SPLIT(_imgs,'+')
		--
		for _i=0;_i<ARRAYSIZE(_imgs);_i++
		{
			imglist += "\q[" + _imgs[_i] + ",OnOpenIMG," + _imgs[_i] + "," + _trueimg[_i] +"]\n"
		}
		
	}
	--
	if delimg == "ON"
	{
		imglist += "\f[size,20]\n\f[color,230, 39, 25]\q[-,delim]\f[color,default] | \f[size,default]"

	}
	else
	{
		imglist += "\f[size,20]\n\q[-,delim] | \f[size,default]"
	}
	--
	if editimg == "ON"
	{
		imglist += "\f[color,230, 39, 25]\q[RENAME,edig]\f[color,default]"

	}
	else
	{
		imglist += "\q[RENAME,edig]"
	}
	--
	imglist += "\n[half]\n\![*]\q[Nevermind,CANCEL] or \![*]\q[Go Back,OnMenuback]\n"
	--
	imglist
}

